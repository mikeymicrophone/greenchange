.section.profile
  - for note in profile.notes
    -unless note.body.blank?
      .note
        %h4.note_title
          = ProfileHelper::NOTE_TYPE_NAMES[ note.note_type ] + ': '
        = truncate( note.body, 400 ) if note.body
  .toolbar
    = link_to 'View full profile', person_profile_url( profile.entity )
