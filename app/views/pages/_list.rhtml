<!-- begin page list -->
<div class="page_list">
<table>
<% if @pages %>
<% @pages.each do |p| %>
<tr>
  <td><%= icon(p.tool_type) %></td>
  <td><%= link_to_page p.title, p %></td>
  <td><%= link_to_page(time_ago_in_words(p.updated_at) + " ago", p) if p.updated_at%></td>
</tr>
<% end %>
<% else %>
<tr><td colspan="10">error: null pages</td></tr>
<% end %>
</table>
</div>
<!-- end page list -->
