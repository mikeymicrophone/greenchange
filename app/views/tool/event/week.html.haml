= render(:partial => "nav")
%h1
  = "#{@date.strftime('%A %B %d, %G')} to #{(@date + 6).strftime('%A %B %d, %G')}"
%p
  = link_to("Previous week: #{(@date-7).strftime('%D')}", :date=>@date-7)
  = link_to("Next week: #{(@date+7).strftime('%D')}", :date=>@date+7)
= render(:partial => "list", :object => @events)
